# Backend environment configuration

# Required
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/circuit_scope

# Optional CORS configuration (comma-separated origins)
CORS_ORIGINS=http://localhost:3000
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOW_METHODS=*
CORS_ALLOW_HEADERS=*

# Application metadata
APP_NAME=Circuit Scope API (Local)
DEBUG=true
API_PREFIX=/api

# KiCad CLI (used for preview rendering)
KICAD_CLI_PATH=kicad-cli
KICAD_CLI_TIMEOUT_SECONDS=120
